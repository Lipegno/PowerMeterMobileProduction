<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">



    <style name="top_menu">
        <item name="android:layout_height">60px</item>
        <item name="android:background">#DFDFDC</item>
    </style>

    <style name="top_menu_item">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">0px</item>
        <item name="android:layout_weight">0.20</item>
    </style>
    <!-- <item name="android:layout_marginBottom">2dp</item> -->

    <style name="top_menu_label">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#4d4d4d</item>
        <item name="android:textSize">24px</item>
    </style>

    <style name="black_separator">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">2dp</item>
        <item name="android:background">#000000</item>
    </style>

    <style name="eventsLabels">
        <item name="android:textSize">9pt</item>
        <item name="android:textColor">#000000</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:padding">10dp</item>
    </style>


    <style name="white_separator">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">2dp</item>
        <item name="android:background">#ffffff</item>
    </style>

    <style name="table_row">
        <item name="android:textColor">#000000</item>
        <item name="android:layout_height">60px</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:textSize">9pt</item>
        <item name="android:paddingTop">5px</item>
    </style>

    <style name="table_item">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15pt</item>
    </style>

    <style name="table_item_label">
        <item name="android:layout_height">40px</item>
        <item name="android:layout_width">160dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">9pt</item>
        <item name="android:paddingLeft">30px</item>
        <item name="android:gravity">center_vertical</item>
    </style>



    <style name="text_box_date">
        <item name="android:layout_height">40px</item>
        <item name="android:layout_width">150px</item>
        <item name="android:textColor">#000000</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="table_item_value">
        <item name="android:layout_height">40px</item>
        <item name="android:layout_width">200px</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">8pt</item>
        <item name="android:paddingLeft">3px</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="info_text">
        <item name="android:textSize">8pt</item>
        <item name="android:paddingLeft">5px</item>
        <item name="android:textColor">#fffafa</item>
    </style>


    <style name="popUpLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:layout_margin">6dp</item>
    </style>


    <style name="summary_table">
        <item name="android:layout_margin">1dp</item>
        <item name="android:layout_width">0px</item>
        <item name="android:background">#C8C5BC</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#4d4d4d</item>
    </style>

    <style name="summary_table_item">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#4d4d4d</item>
    </style>
    <style name="prod_legend">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_weight">0.89</item>
        <item name="android:layout_margin">6dp</item>
        <item name="android:paddingLeft">7dp</item>
        <item name="android:textSize">7pt</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="popupMainLayout">
        <item name="android:layout_width">440px</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/round_box_white_stroke</item>
    </style>
    <style name="eventsBox">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/round_box_white</item>
        <item name="android:layout_marginTop">15px</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>
    <style name="table_item_colored">
        <item name="android:layout_height">60px</item>
        <item name="android:layout_width">300dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">8pt</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:background">@drawable/round_box</item>
        <item name="android:layout_marginLeft">70px</item>
    </style>
